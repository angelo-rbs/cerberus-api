
enum Genero {
  HOMEM_CIS
  MULHER_CIS
  HOMEM_TRANS
  MULHER_TRANS
  NAO_BINARIO
  OUTRO
}

enum Sexo {
  MASCULINO
  FEMININO
}


model Usuario {
  id String @id @default(uuid())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  deletado Boolean @default(false)
  ativo Boolean @default(true)
  verificado Boolean @default(false)

  nome String
  dataNascimento DateTime?
  sexo Sexo?
  genero Genero?

  email String
  username String?
  senha String
}
